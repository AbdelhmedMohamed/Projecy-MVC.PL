@model Department
@{
    ViewData["Title"] = "Details";
}

<h1>Department Details</h1>
<br />
<br />

<div>

    <div>
        <label class="col-form-label" asp-for="Code"></label>
        <input type="text" asp-for="Code" class="form-control" readonly />
       
    </div>
    <div>
        <label class="col-form-label" asp-for="Name"></label>
        <input type="text" asp-for="Name" class="form-control" readonly />
       
    </div>
    <div>
        <label class="col-form-label" asp-for="DataOfCreation"></label>
        <input asp-for="DataOfCreation" class="form-control" readonly />
    </div>

    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary mr-3 mt-5">Edit</a>
    <a asp-action="Index" class="btn btn-light mt-5">Back To List</a>

</div>
